  "main": "index.js",
  "scripts": {
    "server": "cd server && npm start",
    "user": "cd client\\user && npm start",
    "admin": "cd client\\admin && npm start",
    "dev": "concurrently \"npm run server\" \"npm run user\" \"npm run admin\"",
    "install-root": "npm install",
    "install-client-admin": "cd client\\admin && npm install",
    "install-client-user": "cd client\\user && npm install",
    "install-server": "cd server && npm install",
    "install-all": "npm install -g concurrently && concurrently \"npm run install-root\" \"npm run install-client-admin\" \"npm run install-client-user\" \"npm run install-server\""
  },
